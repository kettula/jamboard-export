{
    "name": "Jamboard Export",
    "version": "1.0",
    "author": "Viljo Malmberg",
    "description": "Export or copy the notes on a Google Jamboard onto the clipboard",
    "permissions": [
        "activeTab",
        "background",
        "clipboardWrite"
    ],
    "browser_action": {},
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "manifest_version": 2          
}